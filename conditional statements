void main()
{
    int m;
    printf("enter marks:");
    scanf("%d",&m);
    if(m<0 || m>100)
    {
        printf("Wrong Entry");
    }
    else if(m>=85)
    {
        printf("\n Grade A");
    }
    else if(m<84&&m>=70)
    {
        printf("\n Grade B");
    }
    else if(m<69&&m>=55)
    {
        printf("\n Grade C");
    }
    else if(m<49&&m>=40)
    {
        printf("\n Grade D");
    }
    else
    {
     printf("\n Grade F");
    }

}
